@model IEnumerable<eHospital.Models.Feedback>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPatient.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Appointment.TIME_SLOT)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RATING)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FEEDBACK1)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Appointment.TIME_SLOT)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RATING)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FEEDBACK1)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.FEEDBACK_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.FEEDBACK_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.FEEDBACK_ID })
        </td>
    </tr>
}

</table>
